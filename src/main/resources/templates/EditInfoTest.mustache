                            <label for="floatingPassword">Название</label>
                        </div>
                        <input type="hidden" name="numquestions" value="{{numquestions}}" class="form-control rounded-4" id="floatingPassword" placeholder="Password">
                        <button class="w-100 mb-2 btn btn-lg rounded-4 btn-primary" type="submit">Обновить</button>
