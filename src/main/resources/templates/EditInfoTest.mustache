                            <input type="text" maxlength="100" name="subject" value="{{subject}}" class="form-control rounded-4" id="floatingInput" placeholder="name@example.com">
                            <label for="floatingInput">Предмет</label>
                        </div>
                        <div class="form-floating mb-3">
                            <input type="number" maxlength="2" oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" name="numclass" value="{{numclass}}" class="form-control rounded-4" id="floatingPassword" placeholder="Password">
                            <label for="floatingPassword">Класс</label>
                        </div>
                        <div class="form-floating mb-3">
